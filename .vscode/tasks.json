{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Cargo Build (debug)",
		},
		{
			"type": "cargo",
			"command": "build",
			"args": ["-r"],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Cargo Build (release)",
		},
		{
			"type": "cargo",
			"command": "clean",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Clean Workspace",
		}
	]
}